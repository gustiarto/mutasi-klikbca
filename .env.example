# Example .env file for mutasi-klikbca
# Copy this file to .env and fill with your credentials and config

# KlikBCA credentials
KLIKBCA_USER_ID=your_user_id
KLIKBCA_PIN=your_pin
WEBHOOK_URL=https://your-webhook-url.com/endpoint

# Webhook Basic Auth (optional)
WEBHOOK_BASIC_USER=your_webhook_user
WEBHOOK_BASIC_PASS=your_webhook_pass

# API authentication token
AUTH_TOKEN=klikbca-secret-token

# Port for Express server (optional, default 3040)
PORT=3040

# Cron interval (in cron format, default: every 10 minutes)
CRON_INTERVAL=*/10 * * * *